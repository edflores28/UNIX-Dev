CENTDIR=./conv_to_cent
FAHRDIR=./conv_to_fahr

it:
	$(MAKE) -C $(CENTDIR) it
	$(MAKE) -C $(FAHRDIR) it

clean:
	$(MAKE) -C $(CENTDIR) clean
	$(MAKE) -C $(FAHRDIR) clean 
	
install: 
	$(MAKE) -C $(CENTDIR) install
	$(MAKE) -C $(FAHRDIR) install

depend: 
	$(MAKE) -C $(CENTDIR) depend
	$(MAKE) -C $(FAHRDIR) depend 
